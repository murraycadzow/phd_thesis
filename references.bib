journal = {BMJ Open},
number = {11},
pages = {e003620},
pmid = {24247326},
title = {{Low uric acid levels in patients with Parkinson's disease: evidence from meta-analysis}},
url = {http://bmjopen.bmj.com/lookup/doi/10.1136/bmjopen-2013-003620},
volume = {3},
