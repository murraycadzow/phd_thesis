pages = {519--25},
pmid = {21552263},
title = {{Genome partitioning of genetic variation for complex traits using common SNPs.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21552263 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4295936},
