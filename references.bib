isbn = {1537-1719 (Electronic)$\backslash$r0737-4038 (Linking)},
issn = {1537-1719},
journal = {Mol Biol Evol},
keywords = {*Haplotypes,*Software,Algorithms,Chromosomes, Human, Pair 22/*genetics,Genome,Humans,Selection, Genetic},
number = {10},
pages = {2824--2827},
pmid = {25015648},
title = {{selscan: an efficient multithreaded program to perform EHH-based scans for positive selection}},
