keywords = {Capra hircus,Morocco,WGS,genomic diversity,indigenous populations,population g,population genomics,selection signatures},
number = {April},
pages = {1--14},
title = {{Characterizing neutral genomic diversity and selection signatures in indigenous populations of Moroccan goats ( Capra hircus ) using WGS data}},
url = {http://www.frontiersin.org/Journal/Abstract.aspx?s=620{\&}name=livestock{\_}genomics{\&}ART{\_}DOI=10.3389/fgene.2015.00107},
volume = {6},
