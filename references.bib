journal = {BMC genetics},
keywords = {Cattle,Double muscle,Geographic origin,Polledness,Selection signatures,Selective sweeps,Sheep,cattle,double muscle,geographic origin,polledness,selection signatures,selective sweeps,sheep},
number = {1},
pages = {34},
