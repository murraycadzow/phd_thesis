publisher = {Arthritis Research {\&} Therapy},
title = {{Performance of gout definitions for genetic epidemiological studies: analysis of UK Biobank}},
url = {http://www.embase.com/search/results?subaction=viewrecord{\&}from=export{\&}id=L613886443{\%}5Cnhttp://dx.doi.org/10.1002/art.39977 http://arthritis-research.biomedcentral.com/articles/10.1186/s13075-017-1390-1},
volume = {19},
year = {2017}
}

