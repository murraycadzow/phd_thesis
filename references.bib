doi = {10.1038/ng.2352},
file = {:home/murraycadzow/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Okada et al. - 2012 - Meta-analysis identifies multiple loci associated with kidney function–related traits in east Asian populations.pdf:pdf},
isbn = {8135449550},
issn = {1061-4036},
journal = {Nature Genetics},
number = {8},
pages = {904--909},
pmid = {22797727},
publisher = {Nature Publishing Group},
title = {{Meta-analysis identifies multiple loci associated with kidney function–related traits in east Asian populations}},
url = {http://dx.doi.org/10.1038/ng.2352},
